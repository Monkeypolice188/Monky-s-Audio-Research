 <Unk07>null_sound</Unk07>
 <Unk08 value="20" />
 <ScrapePitch>collision_scrape_pitch</ScrapePitch>
 <ScrapeVolume>collision_car_scrape_volume</ScrapeVolume>
 <SlowScrapeVolume>collision_vehicle_slow_scrape_volume</SlowScrapeVolume>
 <ScrapeImpactVolume>collision_impact_volume</ScrapeImpactVolume>
 <SlowScrapeImpactVolume>collision_impact_volume</SlowScrapeImpactVolume>
 <Unk14>collision_impact_start_offset_boat</Unk14>
